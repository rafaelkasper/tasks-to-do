{
  "expo": {
    "name": "taskstodo",
    "slug": "taskstodo",
    "scheme": "taskstodo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.rafaelkasper.taskstodo"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.rafaelkasper.taskstodo"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Permita que $(PRODUCT_NAME) acesse sua galeria de fotos",
          "cameraPermission": "Permita que $(PRODUCT_NAME) acesse sua c√¢mera"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "2c163af2-1b0d-4da1-8757-0b0c47587a7b"
      }
    }
  }
}
